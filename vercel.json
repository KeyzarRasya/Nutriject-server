{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" },
      { "src": "src/routes/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.js" }
    ],
    "functions": {
      "src/routes/*.js": {
        "maxDuration": 20
      }
    }
  }
  